<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "OneGame" table = "onegame">

        <meta attribute = "class-description">
            This class contains the games details.
        </meta>

        <id name = "id" type = "int" column = "id">
            <generator class="native"/>
        </id>

        <property name = "gameId" column = "gameId" type = "int"/>
        <property name = "moveId" column = "moveId" type = "int"/>
        <property name = "moveString" column = "moveString" type = "string"/>

    </class>
</hibernate-mapping>